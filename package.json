{
	"name": "ffdc-df17-heroku-compute",
	"version": "0.0.0",
	"description": "Node component of Force.com-Heroku App",
	"scripts": {
		"pretest": "eslint heroku/src",
		"test": "nyc mocha --recursive heroku/src/spec"
	},
	"main": "heroku/src/lib/web.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/financialforcedev/df17-heroku-compute"
	},
	"keywords": [],
	"author": "FinancialForce.com",
	"license": "ISC",
	"dependencies": {
		"amqplib": "0.5.1",
		"body-parser": "1.18.0",
		"debug-plus": "1.2.2",
		"express": "4.15.4",
		"helmet": "3.8.1",
		"is-prime": "1.0.1",
		"jsforce": "1.8.0",
		"lodash": "4.17.4"
	},
	"devDependencies": {
		"chai": "3.5.0",
		"chai-as-promised": "7.1.1",
		"chai-sinon": "2.8.1",
		"eslint": "4.5.0",
		"mocha": "3.5.0",
		"nyc": "11.1.0",
		"sinon": "3.2.1",
		"supertest": "3.0.0"
	},
	"engines": {
		"node": "8.9.0"
	},
	"nyc": {
		"check-coverage": true,
		"per-file": true,
		"lines": 100,
		"statements": 100,
		"functions": 100,
		"branches": 100,
		"include": [
			"heroku/src"
		],
		"reporter": [
			"lcov",
			"html",
			"text",
			"text-summary"
		],
		"cache": true,
		"all": true
	}
}
